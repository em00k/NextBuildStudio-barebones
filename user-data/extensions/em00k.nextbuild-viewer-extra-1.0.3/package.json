	"description": "Additional language features for NextBuild Studio",
	"version": "1.0.3",
	"publisher": "em00k",
	"engines": {
		"vscode": "^1.74.0"
	},
	"categories": [
		"Programming Languages"
	],
	"main": "./out/extension.js",
	"contributes": {
		"languages": [
			{
				"id": "nextbuild",
				"aliases": [
					"NextBuild",
					"nextbuild"
				],
				"extensions": [
					".nbld",
					".nbl",
					".bas"
				],
				"configuration": "./language-configuration.json"
			}
		],
		"grammars": [
			{
				"language": "nextbuild",
				"scopeName": "source.nextbuild",
				"path": "./nextbuild.tmLanguage.json"
			}
		]
	},
	"activationEvents": [
		"onLanguage:nextbuild"
	],
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./"
	},
